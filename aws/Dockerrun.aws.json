{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "creasury-bot",
      "image": "{$AWS_ACCOUNT_ID}.dkr.ecr.{$AWS_DEFAULT_REGION}.amazonaws.com/creasury-bot:latest",
      "hostname": "creasury-bot",
      "essential": true,
      "memory": 512,
      "portMappings": [
        {
          "hostPort": 3000,
          "containerPort": 3000
        }
      ]
    }
  ]
}